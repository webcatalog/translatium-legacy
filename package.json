{
  "name": "Translatium",
  "description": "Translate Any Languages like a Pro",
  "version": "13.1.1",
  "main": "public/electron.js",
  "repository": "https://github.com/translatium/translatium",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com"
  },
  "license": "MPL-2.0",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "postinstall": "npx patch-package",
    "start": "cross-env BROWSER=none react-scripts start",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\" -k",
    "build": "react-scripts build",
    "test-ui": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dist": "yarn run build && babel-node packager.js",
    "dist:no-sign": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false yarn dist",
    "lint": "eslint ./src ./public ./packager.js --ext js",
    "test": "mocha"
  },
  "dependencies": {
    "cli-truncate": "2.1.0",
    "electron-is-dev": "1.2.0",
    "electron-settings": "3.2.0",
    "electron-updater": "4.3.3",
    "electron-window-state": "5.0.3",
    "fs-extra": "9.0.1",
    "menubar": "8.0.2",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "blueimp-md5": "2.16.0",
    "chai": "4.2.0",
    "classnames": "2.2.6",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "del": "5.1.0",
    "dotenv": "8.2.0",
    "electron": "8.3.1",
    "electron-builder": "22.7.0",
    "electron-notarize": "0.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "glob": "7.1.6",
    "greek-utils": "1.2.2",
    "mocha": "7.2.0",
    "pouchdb-browser": "7.2.1",
    "pouchdb-quick-search": "1.3.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-countdown": "2.2.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.1",
    "redux": "4.0.5",
